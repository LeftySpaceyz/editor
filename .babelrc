{
  "env": {
    "test": {
      "presets": ["@babel/preset-env"],
      "plugins": [
        "@babel/transform-runtime",
        "@babel/plugin-proposal-optional-chaining"
      ]
    },
    "development": {
      "presets": ["@babel/preset-env"],
      "plugins": [
        "@babel/transform-runtime",
        "@babel/plugin-proposal-optional-chaining",
        ["@babel/plugin-transform-react-jsx", {
          "runtime": "classic"
        }]
      ]
    },
    "production": {
      "presets": ["@babel/preset-env"],
      "plugins": [
        "@babel/transform-runtime",
        "@babel/plugin-proposal-optional-chaining",
        ["@babel/plugin-transform-react-jsx", {
          "runtime": "classic"
        }]        
      ]
    }
  }
}
